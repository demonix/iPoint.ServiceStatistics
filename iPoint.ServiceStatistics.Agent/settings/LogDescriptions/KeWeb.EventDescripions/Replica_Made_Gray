Type=Counter
Source=$host
Category = ServiceInteraction
Counter = Graylisting
instance = server.reg
Value=1
DateTime = {date}
dateFormat = yyyy-MM-dd HH:mm:ss,fff
Regex =^(?<date>\d{4}-\d{1,2}-\d{1,2} \d{1,2}:\d{1,2}:\d{1,2}(?>(?>,|\.)\d+)?).+INFO.*?\s+?Replica\s+?(?:[0-9]{1,3}\.){3}[0-9]{1,3}:\d*?\s+?made\s+?gray
